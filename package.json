    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanhayden/React-NuclearJs-Sequelize-Passport-Boilerplate.git"
  },
