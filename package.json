    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "fbjs": "^0.5.0",
    "history": "^1.13.1",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "material-ui": "^0.14",
    "nodemon": "^1.8.1",
